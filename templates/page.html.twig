<script type="text/javascript">
    var userLanguage = 'en';
    if ( location.host.indexOf('philgo') != -1 ) userLanguage = 'ko';

    function ln(text) {
        var t = text.split(',');
        if ( userLanguage == 'ko' ) document.write(t[1]);
        else document.write(t[0]);
    }
</script>
