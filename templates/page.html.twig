<script type="text/javascript">
    var userLanguage = 'en';
    if ( location.host.indexOf('sonub') != -1 ) userLanguage = 'ko';
    function getval(text) {
        var t = text.split(',');
        if ( userLanguage == 'ko' ) str=t[1];
        else str=t[0];
        return str;
    }
    function setval(text, selector) {
        var str = getval(text);
        document.querySelector(selector).value = str;
    }
    function ln(text, selector) {
        var str = getval(text);
        if ( typeof selector != 'undefined' ) document.querySelector(selector).innerHTML = str;
        else document.write(str)
    }
</script>
{% import dir_macro ~ '/functions.html.twig' as f  %}
{% if sub_theme %}
    {% include dir_sub_theme ~ '/page.html.twig' %}
{% else %}
    {{ page.content }}
{% endif %}
