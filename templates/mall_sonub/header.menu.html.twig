{% set menus = data.category_entity_list %}
<div class='bottom'>
	<div class='inner clearfix'>
	{% for menu in menus %}
		<a class='menu{{ input.category_id == menu.entity.id.value ? ' selected'}}' href='/mall/item/search?category_id={{ menu.entity.id.value }}'>{{ menu.entity.name.value }}<img src='/themes/withcenter/templates/mall_sonub/img/menu_selected.png'/></a>
	{% endfor %}
	</div>
</div>